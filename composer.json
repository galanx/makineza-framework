{
  "name": "makineza/framework",
  "description": "The Makineza PHP Framework package.",
  "keywords": ["framework", "makineza"],
  "license": "MIT",
  "authors": [
    {
      "name": "Albert Feka",
      "email": "feka.albert@gmail.com"
    }
  ],
  "require": {
    "symfony/http-foundation": "^5.0",
    "symfony/routing": "^5.0",
    "symfony/http-kernel": "^5.0",
    "symfony/event-dispatcher": "^5.0",
    "pimple/pimple": "^3.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "Makineza\\": "src/Makineza/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Makineza\\Tests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev"
}